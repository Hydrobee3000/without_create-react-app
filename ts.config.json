{
    "compilerOptions": {
      "lib": ["dom", "dom.iterable", "esnext"], 
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
      "module": "es6",
      "target": "es5",
      "esModuleInterop": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react"
    },
    "include": ["src"],
    "exclude": [
        "./node_modules/**/*"
      ]
  }